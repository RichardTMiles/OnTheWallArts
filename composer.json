  "description": "The best MVC framework",
  "prefer-stable": true,
  "license": "MIT",
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "App\\":"",
      "View\\": "view/",
      "Controller\\": "controller/",
      "Model\\": "model/",
      "Table\\": "table/"
    }
  },
  "authors": [
    {
      "name": "Richard Miles",
      "email": "RichardMiles@Stats.Coach"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/RichardTMiles/CarbonPHP"
    }
  ],
  "require-dev": {
    "richardtmiles/carbonphp" : "dev-carbon#44af044d931a96788db7845d4a0c0b744c44887b",
    "phpunit/phpunit": "^7"
  },
  "require": {
    "php": ">=7.1",
    "google/apiclient": "2.1.3",
    "facebook/graph-sdk": "^5.6.1"
  }
